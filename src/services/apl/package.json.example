{
  "name": "wic-benefits-app",
  "version": "1.0.0",
  "scripts": {
    "// APL Ingestion Scripts": "",
    "ingest:michigan": "ts-node src/services/apl/cli/ingest-michigan.ts",
    "worker:michigan": "ts-node src/services/apl/workers/michigan-sync-worker.ts",

    "// Testing Scripts": "",
    "test:michigan:dry": "npm run ingest:michigan -- --file src/services/apl/test-data/michigan-apl-sample.json --dry-run --verbose",
    "test:michigan:local": "npm run ingest:michigan -- --file test-data/michigan-apl.xlsx --verbose",

    "// Production Scripts": "",
    "sync:all": "npm run worker:michigan"
  },
  "dependencies": {
    "xlsx": "^0.18.5",
    "axios": "^1.6.0",
    "pg": "^8.11.0",
    "cron": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/pg": "^8.10.0",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0"
  }
}
